    <origin xyz="0 -0.05 0.215" rpy="0 0 0" />
    <geometry>
      <box size="0.35 0.02 0.41"/>
    </geometry>
  </collision>
</link>
<joint name = "body_to_mid" type = "fixed">
  <parent link = "body_link"/>
