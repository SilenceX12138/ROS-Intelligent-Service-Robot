      <box size="0.03 0.03 0.75"/>
    </geometry>
  </collision>
</link>
<joint name = "body_to_bf" type = "fixed">
  <parent link = "base_link"/>
  <child link = "bf_link"/>
  <origin rpy="0 0 0" xyz="0 0 0"/> <!--pos-->
</joint>

<link name = "lr_link">
  <collision>
    <origin xyz="0.235 0.585 0.375" rpy="0 0 0" />
    <geometry>
      <box size="0.03 0.03 0.75"/>
    </geometry>
  </collision>
</link>
<joint name = "body_to_lr" type = "fixed">
  <parent link = "base_link"/>
  <child link = "lr_link"/>
  <origin rpy="0 0 0" xyz="0 0 0"/> <!--pos-->
